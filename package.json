{
  "name": "swaglabs-allure-cicd",
  "version": "1.0.0",
  "description": "Playwright + TypeScript + Allure test automation for SwagLabs",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "report": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "report:generate": "allure generate allure-results --clean -o allure-report",
    "report:view": "node scripts/view-report.js"
  },
  "keywords": [
    "playwright",
    "typescript",
    "allure",
    "test-automation",
    "e2e"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "allure-commandline": "^2.24.1",
    "allure-playwright": "^2.10.0",
    "typescript": "^5.3.3"
  }
}
